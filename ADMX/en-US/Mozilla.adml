<?xml version="1.0" encoding="utf-8"?>
<policyDefinitionResources revision="1.0" schemaVersion="1.0">
  <displayName>FirefoxADMX</displayName>
  <description>Firefox ADMX is a way of allowing centrally managed locked and/or default settings in Firefox via Group Policy and Administrative Templates in Active Directory. Firefox ADMX is a continuation of Firefox ADM by Mark Sammons.</description>
  <resources>
    <stringTable>
      <string id="MOZILLA_FIREFOX">Mozilla Firefox</string>
      <string id="MOZILLA_FIREFOX_LOCKED">Locked Preferences</string>
      <string id="MOZILLA_FIREFOX_LOCKED_CUSTOM_HOMEPAGE">Custom Homepage</string>
      <string id="MOZILLA_FIREFOX_LOCKED_CUSTOM_HOMEPAGE_BLANKPAGE">Blank Page</string>
      <string id="MOZILLA_FIREFOX_LOCKED_CUSTOM_HOMEPAGE_CUSTOMPAGE">Custom Homepage</string>
      <string id="MOZILLA_FIREFOX_LOCKED_CUSTOM_HOMEPAGE_DEFAULTPAGE">Default Page</string>
      <string id="MOZILLA_FIREFOX_LOCKED_CUSTOM_USERAGENT">Custom Useragent</string>
      <string id="MOZILLA_FIREFOX_LOCKED_NTLM_TRUSTED_URIS">NTLM Authentication Trusted URIs</string>
      <string id="MOZILLA_FIREFOX_LOCKED_DISABLEDEFAULTCHECK">Disable Default Browser Check</string>
      <string id="MOZILLA_FIREFOX_LOCKED_DISABLEIMPORT">Disable Import Wizard</string>
      <string id="MOZILLA_FIREFOX_LOCKED_DISABLEBROWSERMILESTONE">Disable Browser Miletone Page</string>
      <string id="MOZILLA_FIREFOX_LOCKED_DISABLEUPDATES">Disable Automatic Updates</string>
      <string id="MOZILLA_FIREFOX_LOCKED_DISABLE_DM">Disable Download Manager</string>
      <string id="MOZILLA_FIREFOX_LOCKED_DISABLE_DM_Help">Disables the Download Manager from appearing when you start to download a file.</string>
      <string id="MOZILLA_FIREFOX_LOCKED_DISABLE_PASSWORDS">Disable Password Manager</string>
      <string id="MOZILLA_FIREFOX_LOCKED_CUSTOM_HOMEPAGE_Help">Allows the customization of the homepage. You can choose to blank the home page or set a custom URL.</string>
      <string id="MOZILLA_FIREFOX_LOCKED_CUSTOM_USERAGENT_Help">Allows the customization of the useragent. This overrides the entire useragent sent to the server.</string>
      <string id="MOZILLA_FIREFOX_LOCKED_NTLM_TRUSTED_URIS_Help">Comma-seperated list of FQDNs to allow NTLM authentication.</string>
      <string id="MOZILLA_FIREFOX_LOCKED_SUPRESSUPDATEPAGE">Supress "Firefox Updated" page after update</string>
      <string id="MOZILLA_FIREFOX_LOCKED_SUPRESSUPDATEPAGE_Help">After updating Firefox, by default it display's a "What's New" page.

If you enable this policy, that page will be supressed.</string>
      <string id="MOZILLA_FIREFOX_LOCKED_DISABLEADDONWIZARD">Disable Add-On Wizard</string>
      <string id="MOZILLA_FIREFOX_LOCKED_DISABLEADDONWIZARD_Help">Disables the Add-On Wizard at startup.</string>
      <string id="MOZILLA_FIREFOX_LOCKED_DISABLETELEMETRY">Disable Telemetry</string>
      <string id="MOZILLA_FIREFOX_LOCKED_DISABLETELEMETRY_Help">Disables prompt and disables sending data to Mozilla. Disabling this will enable Telemetry.</string>
      <string id="MOZILLA_FIREFOX_LOCKED_DISABLERIGHTS">Disable "Know your rights"</string>
      <string id="MOZILLA_FIREFOX_LOCKED_DISABLERIGHTS_Help">Disables the "Know your rights..." prompt on first run.</string>
      <string id="MOZILLA_FIREFOX_LOCKED_PROXYSETTINGS">Proxy Settings</string>
      <string id="MOZILLA_FIREFOX_LOCKED_PROXY_SOCKSV4">SOCKS v4</string>
      <string id="MOZILLA_FIREFOX_LOCKED_PROXY_SOCKSV5">SOCKS v5</string>
      <string id="MOZILLA_FIREFOX_LOCKED_PROXY_NOPRX">No proxy</string>
      <string id="MOZILLA_FIREFOX_LOCKED_PROXY_AUTO">Autodetect network configuration</string>
      <string id="MOZILLA_FIREFOX_LOCKED_PROXY_USESYSTEM">Use system configuration</string>
      <string id="MOZILLA_FIREFOX_LOCKED_PROXY_MANUALPRX">Manual proxy configuration</string>
      <string id="MOZILLA_FIREFOX_LOCKED_PROXY_USEPAC">Use auto configuration URL (PAC)</string>	  
    </stringTable>
